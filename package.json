{
    "name": "vscode-opensips",
    "displayName": "vscode-opensips",
    "description": "vscode-opensips",
    "publisher": "Tommy Brecher",
    "version": "1.0.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/tommybrecher/vscode-opensips.git"
    },
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "opensips",
                "aliases": [
                    "OpenSIPs Syntax Highlight for VS Code",
                    "opensips"
                ],
                "extensions": [
                    ".cfg",
                    ".cfg.j2"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "opensips",
                "scopeName": "source.opensips",
                "path": "./syntaxes/opensips.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.jinja": "jinja"
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.32.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4"
    }
}
